@import "../../node_modules/bootstrap/dist/css/bootstrap.css"

html
body
    margin 0
    padding 0
    width 100%
    height 100%

.navbar-nav
    li
        margin-left 20px
    select
        height 30px
        margin 10px

.dg
    z-index 20 !important

.dg.main.a
    margin 0
    select
        color black

select[disabled]
    color grey

canvas, svg
    position absolute
    margin-left 20px

canvas
    z-index 5

svg
    z-index 1

    .land
        fill lightgrey

    .county-boundary
        fill none
        stroke #fff
        stroke-width .5px

    .state-boundary
      fill none
      stroke #fff

table
    border 1px solid #e7e7e7
    background #f8f8f8
    color black
    font-size 11px
    margin auto
    position relative
    z-index 6

th
td
    padding 10px


